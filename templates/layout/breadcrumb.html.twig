{% if breadcrumb %}
  <nav id="ib3-module--ib3-breadcrumb" aria-label="breadcrumb" role="navigation">
    <ol class="breadcrumb bg-white px-0 pt-0">
      {% for item in breadcrumb %}
        {% if item.url %}
          <li class="breadcrumb-item"><a href="{{ item.url }}">{{ item.text }}</a></li>
        {% else %}
          <li class="breadcrumb-item active" aria-current="page">{{ item.text }}</li>
        {% endif %}
      {% endfor %}
    </ol>
  </nav>
{% endif %}
